<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/>
        </trust-anchors>
    </base-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">madrastak.moe.edu.eg</domain>
        <domain includeSubdomains="true">madrasetna.net</domain>
        <domain includeSubdomains="true">mfm-student.madrasetna.net</domain>
        <domain includeSubdomains="true">minio-proxy.madrasetna.net</domain>
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/>
        </trust-anchors>
    </domain-config>
</network-security-config>
